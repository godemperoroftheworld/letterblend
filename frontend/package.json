  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 8080",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "npx eslint --ext .ts,*.vue",
    "format": "npx prettier . --write --ignore-unknown",
    "prepare": "husky",
    "commit": "cz"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@tanstack/query-persist-client-core": "^5.51.1",
    "@tanstack/query-sync-storage-persister": "^5.51.1",
    "@tanstack/react-query-persist-client": "^5.51.1",
    "@tanstack/vue-query": "^5.51.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "socket.io-client": "^4.7.5",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "fuse.js": "^7.0.0",
    "lodash.intersection": "^4.4.0",
    "tailwindcss": "^3.3.0",
    "theme-change": "^2.5.0",
    "vite": "^5.3.1",
    "vue": "^3.4.29",
    "vue-fuse": "^4.1.1",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.17.6",
    "@types/lodash.intersection": "^4.4.9",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^4.12.10",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "standard-version": "^9.5.0",
    "typescript": "^5",
    "vue-tsc": "^2.0.26"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
